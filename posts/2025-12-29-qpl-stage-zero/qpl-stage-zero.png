<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 400">
  <!-- Background -->
  <rect width="800" height="400" fill="#f8f9fa"/>

  <!-- Title -->
  <text x="400" y="50" font-family="Arial, sans-serif" font-size="26" font-weight="bold" fill="#2c3e50" text-anchor="middle">
    Stage Zero: Relations-First Quantum Programming
  </text>

  <!-- Traditional approach (left side) -->
  <g transform="translate(150, 150)">
    <text x="0" y="-50" font-family="Arial, sans-serif" font-size="18" fill="#7f8c8d" text-anchor="middle" font-weight="bold">Gate-Based</text>

    <!-- Three separate qubits -->
    <circle cx="0" cy="0" r="25" fill="#3498db" opacity="0.7"/>
    <text x="0" y="5" font-family="Arial, sans-serif" font-size="14" fill="white" text-anchor="middle">q₀</text>

    <circle cx="0" cy="60" r="25" fill="#3498db" opacity="0.7"/>
    <text x="0" y="65" font-family="Arial, sans-serif" font-size="14" fill="white" text-anchor="middle">q₁</text>

    <circle cx="0" cy="120" r="25" fill="#3498db" opacity="0.7"/>
    <text x="0" y="125" font-family="Arial, sans-serif" font-size="14" fill="white" text-anchor="middle">q₂</text>

    <text x="0" y="170" font-family="Arial, sans-serif" font-size="12" fill="#7f8c8d" text-anchor="middle">Objects + Gates</text>
  </g>

  <!-- Arrow -->
  <g transform="translate(400, 250)">
    <line x1="-150" y1="0" x2="150" y2="0" stroke="#e74c3c" stroke-width="3" marker-end="url(#arrowhead2)"/>
    <text x="0" y="-10" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#e74c3c" text-anchor="middle">QPL</text>
  </g>

  <!-- Relations-first approach (right side) -->
  <g transform="translate(650, 150)">
    <text x="0" y="-50" font-family="Arial, sans-serif" font-size="18" fill="#7f8c8d" text-anchor="middle" font-weight="bold">Relations-First</text>

    <!-- Entangled system as network -->
    <circle cx="-30" cy="-30" r="20" fill="#9b59b6" opacity="0.8"/>
    <circle cx="30" cy="-30" r="20" fill="#9b59b6" opacity="0.8"/>
    <circle cx="0" cy="40" r="20" fill="#9b59b6" opacity="0.8"/>

    <!-- Connection lines showing entanglement -->
    <line x1="-20" y1="-20" x2="20" y2="-20" stroke="#9b59b6" stroke-width="2.5"/>
    <line x1="-15" y1="-15" x2="-5" y2="30" stroke="#9b59b6" stroke-width="2.5"/>
    <line x1="15" y1="-15" x2="5" y2="30" stroke="#9b59b6" stroke-width="2.5"/>

    <!-- Labels -->
    <text x="-30" y="-25" font-family="Arial, sans-serif" font-size="11" fill="white" text-anchor="middle">0</text>
    <text x="30" y="-25" font-family="Arial, sans-serif" font-size="11" fill="white" text-anchor="middle">1</text>
    <text x="0" y="45" font-family="Arial, sans-serif" font-size="11" fill="white" text-anchor="middle">2</text>

    <text x="0" y="90" font-family="Arial, sans-serif" font-size="12" fill="#9b59b6" text-anchor="middle" font-weight="bold">QuantumRelation</text>
    <text x="0" y="170" font-family="Arial, sans-serif" font-size="12" fill="#7f8c8d" text-anchor="middle">Entanglement First</text>
  </g>

  <!-- Arrow marker -->
  <defs>
    <marker id="arrowhead2" markerWidth="10" markerHeight="10" refX="9" refY="3" orient="auto">
      <polygon points="0 0, 10 3, 0 6" fill="#e74c3c"/>
    </marker>
  </defs>

  <!-- Subtitle -->
  <text x="400" y="370" font-family="Arial, sans-serif" font-size="15" fill="#7f8c8d" text-anchor="middle">
    Entanglement as Primitive • Questions over Measurements • Relations over Objects
  </text>
</svg>
